{
  "name": "slotmachine",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "watchman watch-del-all && react-native run-android",
    "ios": "watchman watch-del-all && react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle-untested": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/",
    "build-android-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "build-android-release": "cd android && ./gradlew assembleRelease && cd .."
  },
  "dependencies": {
    "expo": "^48.0.0",
    "expo-av": "~13.2.1",
    "Math": "^0.0.1-security",
    "react": "18.2.0",
    "react-native": "0.71.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}